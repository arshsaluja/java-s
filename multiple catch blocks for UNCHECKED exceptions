import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		
		System.out.println("start");
		System.out.println("Progress");
        String s="welcome";
        int n=0;
        try{
        
        n= Integer.parseInt(s);//NumberFormatException
        }
        catch(ArithmeticException e){
            System.out.println("ArithmeticException handled sucessfully");
        }
        catch(NumberFormatException e){
            System.out.println("NumberFormatException handled sucessfully");
        }
        catch(NullPointerException e){
            System.out.println(" NullPointerException handled sucessfully");
        }
        catch(ArrayIndexOutOfBoundsException e){
            System.out.println("ArrayIndexOutOfBoundsException handled sucessfully");
        }
        System.out.println(n+100);
		System.out.println("completed");
		System.out.println("exited");
		
	}
}
